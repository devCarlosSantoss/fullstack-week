// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// @id - Chave primária
// @default(uuid()) - Gera id aleatório
model Category {
  id String @id @default(uuid())
  name String
  slug String
  imageUrl String
}
